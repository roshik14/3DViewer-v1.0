<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <object id="about_window" class="GtkMessageDialog">
        <property name="title">About</property>
        <property name="default_width">300</property>
        <property name="default_height">200</property>
        <property name="resizable">False</property>
        <property name="can_focus">False</property>
        <property name="title">Object info</property>
        <property name="modal">True</property>
        <child>
            <object id="message_vbox1" class="GtkBox">
                <property name="visible">True</property>
                <property name="orientation">vertical</property>
                <property name="spacing">12</property>
                <child>
                    <object id="message_hbox1" class="GtkBox">
                        <property name="visible">True</property>
                        <property name="orientation">horizontal</property>
                        <property name="spacing">12</property>
                        <property name="margin_start">60</property>
                        <property name="margin_end">60</property>
                        <property name="margin_top">30</property>
                        <child>
                            <object id="name_obj_label" class="GtkLabel">
                                <property name="label">Object name</property>
                            </object>
                        </child>
                        <child>
                            <object id="equal_label1" class="GtkLabel">
                                <property name="label">=</property>
                                <property name="margin_start">37</property>
                            </object>
                        </child>
                        <child>
                            <object id="name_obj_label_value" class="GtkLabel">
                                <property name="label"></property>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object id="message_hbox2" class="GtkBox">
                        <property name="visible">True</property>
                        <property name="orientation">horizontal</property>
                        <property name="spacing">12</property>
                        <property name="margin_start">60</property>
                        <property name="margin_end">60</property>
                        <child>
                            <object id="vertices_label" class="GtkLabel">
                                <property name="label">Count of vertices</property>
                            </object>
                        </child>
                        <child>
                            <object id="equal_label2" class="GtkLabel">
                                <property name="label">=</property>
                            </object>
                        </child>
                        <child>
                            <object id="vertices_value" class="GtkLabel">
                                <property name="label">Test2</property>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object id="message_hbox3" class="GtkBox">
                        <property name="visible">True</property>
                        <property name="orientation">horizontal</property>
                        <property name="spacing">12</property>
                        <property name="margin_start">60</property>
                        <property name="margin_end">60</property>
                        <property name="margin_bottom">30</property>
                        <child>
                            <object id="edges_label" class="GtkLabel">
                                <property name="label">Count of edges</property>
                            </object>
                        </child>
                        <child>
                            <object id="equal_label3" class="GtkLabel">
                                <property name="label">=</property>
                                <property name="margin_start">15</property>
                            </object>
                        </child>
                        <child>
                            <object id="edges_value" class="GtkLabel">
                                <property name="label">Test3</property>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object id="close_btn" class="GtkButton">
                        <property name="label">CLOSE</property>
                        <property name="margin_start">60</property>
                        <property name="margin_end">60</property>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>
